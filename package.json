{
  "name": "comet",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:vercel": "vercel dev --listen 5173",
    "dev:local": "npm run db:start && vite",
    "server": "tsx watch server/index.ts",
    "server:prod": "tsx server/index.ts",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "docker:dev": "docker-compose up",
    "docker:dev:build": "docker-compose up --build",
    "docker:dev:down": "docker-compose down",
    "docker:prod": "docker-compose -f docker-compose.prod.yml up",
    "docker:prod:build": "docker-compose -f docker-compose.prod.yml up --build",
    "docker:prod:down": "docker-compose -f docker-compose.prod.yml down",
    "db:start": "npx supabase start",
    "db:stop": "npx supabase stop",
    "db:reset": "npx supabase db reset",
    "db:status": "npx supabase status",
    "db:push": "npx supabase db push",
    "db:diff": "npx supabase db diff",
    "db:migrate:new": "npx supabase migration new",
    "setup:local": "bash scripts/setup-local-env.sh",
    "seed:coaches": "node scripts/seed-coaches.js",
    "db:run-sql": "node scripts/run-sql.js"
  },
  "dependencies": {
    "@stripe/react-stripe-js": "^5.4.1",
    "@stripe/stripe-js": "^8.6.0",
    "@supabase/supabase-js": "^2.90.1",
    "@vercel/node": "^5.5.16",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.21.2",
    "intuit-oauth": "^4.2.2",
    "micro": "^10.0.1",
    "node-quickbooks": "^2.0.47",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.10.1",
    "stripe": "^20.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "pg": "^8.17.2",
    "sass-embedded": "^1.93.3",
    "tsx": "^4.19.2",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4"
  }
}
